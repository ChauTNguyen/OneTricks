  {
    "name": "onetricks.net",
    "reason": {
      "react-jsx": 2
    },
    "bs-dependencies": [
      "reason-react",
      "reason-js",
      "reason-reroute",
      "bs-fetch",
      "@glennsl/bs-json",
      "bs-css"
    ],
    "bs-dev-dependencies": [
      "@glennsl/bs-jest"
    ],
    "sources": [
      {
        "dir": "src",
        "subdirs": true
      },
      {
        "dir": "__tests__",
        "type": "dev"
      }
    ],
    "package-specs": [
      {
        "module": "commonjs",
        "in-source": true
      }
    ],
    "refmt": 3,
    "suffix": ".js", /* use this instead of .bs.js, reason-scripts relies on .js */
    "bsc-flags": [
      "-bs-no-version-header",
      "-bs-super-errors"
    ]
  }