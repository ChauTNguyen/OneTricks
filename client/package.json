{
  "name": "one-tricks",
  "version": "0.1.0",
  "private": true,
  "proxy": "http://one-tricks-api:3001/",
  "devDependencies": {
    "bs-platform": "^2.1.0",
    "concurrently": "3.5.1",
    "enzyme": "^2.4.1",
    "react-addons-test-utils": "^15.4.0",
    "react-scripts": "1.1.1"
  },
  "dependencies": {
    "babel-polyfill": "^6.23.0",
    "react": "^15.4.0",
    "react-addons-perf": "^15.4.2",
    "react-dom": "^15.4.0",
    "reason-js": "^0.3.0",
    "reason-react": "^0.3.1",
    "reason-reroute": "^0.0.4",
    "recompose": "^0.24.0"
  },
  "scripts": {
    "start": "react-scripts start",
    "dev": "concurrently \"yarn buckle:up\" \"yarn start\"",
    "build": "mv -v ./node_modules/reason-react/lib/js/src/* ./node_modules/reason-react/src/; react-scripts build; mv build/index.html build/200.html",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "buckle:up": "bsb -make-world -w",
    "buckle:clean": "bsb -clean-world"
  }
