{
  "name": "one-tricks",
  "version": "0.1.0",
  "private": true,
  "proxy": "http://one-tricks-api:3001/",
  "devDependencies": {
    "@glennsl/bs-jest": "^0.4.2",
    "bs-platform": "^2.1.0",
    "webpack-bundle-analyzer": "^2.11.1"
  },
  "dependencies": {
    "@glennsl/bs-json": "^1.2.0",
    "babel-polyfill": "^6.26.0",
    "bs-css": "^6.6.0",
    "react": "^15.4.0",
    "react-addons-perf": "^15.4.2",
    "react-dom": "^15.4.0",
    "reason-js": "^0.3.0",
    "reason-react": "^0.3.4",
    "reason-reroute": "^0.0.4",
    "reason-scripts": "^0.8.0"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build && mv build/index.html build/200.html && cp CNAME build/CNAME",
    "test": "jest",
    "watch:jest": "jest --coverage --watchAll",
    "eject": "react-scripts eject",
    "prepare": "npm link bs-platform",
    "analyze-bundle": "node ./build.js",
    "deploy": "cd build && surge && cd -",
    "buckle:up": "bsb -make-world -w",
    "buckle:down": "bsb -clean-world",
    "buckle:reset": "bsb -clean-world && bsb -make-world"
  }
}