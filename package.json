{
  "name": "onetricks.net",
  "version": "0.0.1",
  "private": true,
  "babel": {
    "presets": [
      "es2015",
      "stage-0"
    ]
  },
  "dependencies": {
    "babel-cli": "6.14.0",
    "babel-core": "6.14.0",
    "babel-preset-es2015": "6.14.0",
    "babel-preset-stage-0": "6.5.0",
    "compression": "^1.6.2",
    "express": "4.13.3",
    "kindred-api": "^2.0.67",
    "mongoose": "^4.8.4",
    "react": "^15.4.2",
    "react-dom": "^15.4.2",
    "riot-api-client": "^2.0.2"
  },
  "scripts": {
    "start": "concurrently \"npm run server\" \"npm run client\"",
    "server": "babel-node server.js",
    "client": "babel-node start-client.js",
    "dev": "echo \"This command has been deprecated. Use 'npm start'\" && exit 1",
    "build": "cd client && npm run build && ..",
    "deploy": "git push https://git.heroku.com/onetricks.git master",
    "lint": "node_modules/.bin/eslint client",
    "flow": "flow --show-all-errors client/src",
    "test": "yarn eslint && yarn flow"
  },
  "devDependencies": {
    "concurrently": "^3.1.0",
    "dotenv": "^4.0.0",
    "eslint": "^3.12.2",
    "eslint-config-airbnb": "^13.0.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^2.2.3",
    "eslint-plugin-react": "^6.8.0",
    "flow": "^0.2.3",
    "flow-bin": "^0.49.1"
  }
}
